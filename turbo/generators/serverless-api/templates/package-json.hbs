{
  "name": "@dbbs/{{ name }}",
  "version": "2.0.0",
  "description": "DBBs {{ name }}",
  "type": "module",
  "main": "./dist/lambda.js",
  "types": "./dist/lambda.d.ts",
  "scripts": {
    "build": "npx tsc --build tsconfig.json",
    "clean": "npx rimraf .turbo node_modules dist bin coverage *.tsbuildinfo || exit 0",
    "dev": "yarn build && npx sls offline start --noTimeout --stage local",
    "lint": "npx eslint \"src/**/*.ts*\" --fix",
    "start": "npx nest start",
    "pretest": "yarn build",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit --coverage --verbose --testTimeout=10000000 --detectOpenHandles",
    "watch": "npx tsc -w --preserveWatchOutput",
    "start:localstack": "yarn build && npx sls offline start --noTimeout --stage local",
    "docs:generate": "npx compodoc -p tsconfig.json",
    "docs:serve": "yarn docs:generate -s",
    "check-types": "npx tsc -p ./tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "@dbbs/common": "*",
    "@dbbs/feature-config": "*",
    "@dbbs/nestjs-module-health": "*",
    "@dbbs/nestjs-module-logger": "*",
    "@dbbs/s3-log-transport": "*",
    "@middy/core": "4.7.0",
    "@middy/do-not-wait-for-empty-event-loop": "4.7.0",
    "@middy/http-security-headers": "4.7.0",
    "@middy/input-output-logger": "4.7.0",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/terminus": "10.2.3",
    "@vendia/serverless-express": "4.12.6",
    "aws-lambda": "1.0.7",
    "express": "4.19.2",
    "form-data": "4.0.0",
    "helmet": "7.1.0",
    "jwks-rsa": "3.1.0",
    "nestjs-cls": "4.3.0",
    "ow": "1.1.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pino-cloudwatch": "0.7.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@dbbs/tsconfig": "*"
  }
}
