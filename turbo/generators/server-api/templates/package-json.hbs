{
  "name": "@dbbs/{{ name }}",
  "version": "2.0.0",
  "description": "DBBs {{ name }}",
  "type": "module",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "npx nest build",
    "clean": "npx rimraf .turbo node_modules dist bin coverage *.tsbuildinfo || exit 0",
    "dev": "npx nest start --watch",
    "format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "npx eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "npx nest start",
    "start:debug": "npx nest start --debug --watch",
    "start:pm2": "node dist/cluster.js",
    "start:prod": "node dist/main",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit --coverage --verbose --testTimeout=10000000 --detectOpenHandles",
    "test:cov": "npx jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx jest --config ./test/jest-e2e.json",
    "test:watch": "npx jest --watch",
    "docs:generate": "npx compodoc -p tsconfig.json",
    "docs:serve": "yarn docs:generate -s",
    "check-types": "npx tsc -p ./tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "@dbbs/common": "*",
    "@dbbs/feature-config": "*",
    "@dbbs/nestjs-module-health": "*",
    "@dbbs/nestjs-module-logger": "*",
    "@dbbs/s3-log-transport": "*",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/passport": "11.0.5",
    "@nestjs/terminus": "10.2.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pino": "9.6.0",
    "pino-cloudwatch": "0.7.0",
    "pm2": "5.3.1"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@dbbs/tsconfig": "*",
    "jest-mock-extended": "3.0.4"
  }
}
