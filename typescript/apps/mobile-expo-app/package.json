{
  "name": "@dbbs/mobile-expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "npx rimraf .serverless .turbo node_modules dist bin coverage && find . -name '*.tsbuildinfo' -type f -delete || exit 0",
    "test": "npx jest --coverage --verbose",
    "lint": "npx eslint . --ext ts,tsx --max-warnings 0 --fix",
    "build": "yarn storybook-generate",
    "start": "NODE_ENV=development expo start",
    "start:prod": "NODE_ENV=production expo start",
    "start:clean": "yarn watchman-refresh && NODE_ENV=development yarn start --clear",
    "watchman-refresh": "watchman watch-del-all && watchman watch-project .",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android:dev-local": "npx expo run:android --variant devDebug",
    "android:prod-local": "npx expo run:android --variant prodDebug",
    "android:dev-eas": "chmod +x eas_hooks/*.sh && NODE_ENV=development ./eas_hooks/eas-run-with-env.sh eas build --profile dev --platform android --local",
    "android:prod-eas": "chmod +x eas_hooks/*.sh && NODE_ENV=production ./eas_hooks/eas-run-with-env.sh eas build --profile prod --platform android --local",
    "android:dev-ci": "NODE_ENV=development eas build --profile dev --platform android",
    "android:prod-ci": "NODE_ENV=production eas build --profile prod --platform android",
    "ios:dev-local": "npx expo run:ios --scheme 'DBBSExpo Dev' --device 'iPhone 16 Pro'",
    "ios:prod-local": "npx expo run:ios --scheme 'DBBSExpo Prod' --device 'iPhone 16 Pro'",
    "ios:dev-eas": "chmod +x eas_hooks/*.sh && NODE_ENV=development ./eas_hooks/eas-run-with-env.sh eas build --profile dev --platform ios --local",
    "ios:prod-eas": "chmod +x eas_hooks/*.sh && NODE_ENV=production ./eas_hooks/eas-run-with-env.sh eas build --profile prod --platform ios --local",
    "ios:dev-ci": "NODE_ENV=development eas build --profile dev --platform ios",
    "ios:prod-ci": "NODE_ENV=production eas build --profile prod --platform ios",
    "web": "expo start --web",
    "prebuild:dev": "echo 'y' | npx expo prebuild --no-install --clean && NODE_ENV=development tsx ./scripts/prebuild",
    "prebuild:prod": "echo 'y' | npx expo prebuild --no-install --clean && NODE_ENV=production tsx ./scripts/prebuild",
    "eas-build-pre-install": "chmod +x eas_hooks/*.sh && ./eas_hooks/eas-build-pre-install.sh",
    "eas-build-post-install": "./eas_hooks/eas-build-post-install.sh",
    "build:ios-dev": "detox build --configuration ios.sim.dev",
    "build:ios-prod": "detox build --configuration ios.sim.prod",
    "build:android-dev": "detox build --configuration android.emu.dev",
    "build:android-prod": "detox build --configuration android.emu.prod",
    "build:android-ci": "detox build --configuration android.emu.ci",
    "test:ios-dev": "detox test --configuration ios.sim.dev",
    "test:ios-prod": "detox test --configuration ios.sim.prod",
    "test:android-dev": "detox test --configuration android.emu.dev",
    "test:android-prod": "detox test --configuration android.emu.prod",
    "test:android-ci": "detox test --configuration android.emu.ci --headless --record-logs all",
    "e2e:ios": "yarn build:ios-prod && yarn test:ios-prod",
    "e2e:android": "yarn build:android-prod && yarn test:android-prod",
    "e2e:android-ci": "yarn build:android-ci && yarn test:android-ci",
    "e2e": "yarn e2e:ios && yarn e2e:android",
    "e2e:ci": "yarn e2e:android-ci",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "STORYBOOK_ENABLED=true && expo start",
    "storybook:ios": "STORYBOOK_ENABLED=true && expo start --ios",
    "storybook:android": "STORYBOOK_ENABLED=true && expo start --android",
    "check-types": "npx tsc -p ./tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "@dbbs/mobile-common": "*",
    "@dbbs/mobile-components": "*",
    "@dbbs/mobile-expo-notifications": "*",
    "@dbbs/mobile-firebase": "*",
    "@dbbs/mobile-iap": "*",
    "@dbbs/react-localization-provider": "*",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/slider": "4.5.6",
    "@react-native-firebase/app": "21.0.0",
    "@react-native-firebase/messaging": "21.0.0",
    "@react-native-firebase/remote-config": "21.0.0",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "@sentry/react-native": "5.36.0",
    "expo": "51.0.39",
    "expo-build-properties": "0.12.5",
    "expo-constants": "16.0.2",
    "expo-dev-client": "4.0.29",
    "expo-notifications": "0.28.19",
    "expo-status-bar": "1.12.1",
    "expo-updates": "0.25.28",
    "react-native": "0.75.5",
    "react-native-branch": "6.5.0",
    "react-native-gesture-handler": "2.24.0",
    "react-native-iap": "12.16.2",
    "react-native-localize": "3.4.1",
    "react-native-mmkv": "2.12.2",
    "react-native-pager-view": "6.7.0",
    "react-native-reanimated": "3.17.1",
    "react-native-safe-area-context": "4.14.1",
    "react-native-screens": "3.36.0",
    "react-native-svg": "15.11.2",
    "react-native-tab-view": "3.5.2",
    "react-native-vector-icons": "10.2.0",
    "react-native-web": "0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@config-plugins/detox": "8.0.0",
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@dbbs/tsconfig": "*",
    "@expo/metro-runtime": "3.2.3",
    "@react-native/babel-preset": "0.75.5",
    "@react-native/eslint-config": "0.75.5",
    "@react-native/metro-config": "0.75.5",
    "@react-native/typescript-config": "0.75.5",
    "@storybook/addon-actions": "8.1.11",
    "@storybook/addon-controls": "8.1.11",
    "@storybook/addon-essentials": "8.1.11",
    "@storybook/addon-links": "8.1.11",
    "@storybook/addon-ondevice-actions": "7.6.20",
    "@storybook/addon-ondevice-backgrounds": "7.6.20",
    "@storybook/addon-ondevice-controls": "7.6.20",
    "@storybook/addon-ondevice-notes": "7.6.20",
    "@storybook/addon-react-native-web": "0.0.24",
    "@storybook/react": "8.1.11",
    "@storybook/react-native": "7.6.20",
    "@storybook/react-webpack5": "8.1.11",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "12.5.3",
    "babel-loader": "9.1.3",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-react-docgen-typescript": "1.5.1",
    "babel-plugin-react-native-web": "0.19.13",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "babel-preset-expo": "11.0.15",
    "detox": "20.34.4",
    "jest": "29.7.0",
    "jest-expo": "51.0.4",
    "storybook": "8.1.11",
    "tsx": "4.19.1"
  }
}
