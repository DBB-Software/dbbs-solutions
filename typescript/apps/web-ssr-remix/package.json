{
  "name": "@dbbs/web-ssr-remix",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "npx rimraf .serverless .turbo node_modules dist bin coverage && find . -name '*.tsbuildinfo' -type f -delete || exit 0",
    "lint": "npx eslint . --ext ts,tsx --max-warnings 0 --fix",
    "dev": "remix vite:dev",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "build": "remix vite:build",
    "test": "npx jest --coverage --verbose",
    "start": "remix-serve build/server/index.js"
  },
  "dependencies": {
    "@dbbs/tailwind-components": "*",
    "@remix-run/node": "2.12.1",
    "@remix-run/react": "2.12.1",
    "@remix-run/serve": "2.12.1",
    "isbot": "4.1.0",
    "match-sorter": "6.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sort-by": "1.2.0",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@dbbs/cypress": "*",
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@remix-run/dev": "2.12.1",
    "@remix-run/testing": "2.12.1",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.1",
    "vite-tsconfig-paths": "4.3.1"
  }
}
