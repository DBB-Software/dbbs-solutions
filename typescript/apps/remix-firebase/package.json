{
  "name": "@dbbs/remix-firebase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "npx rimraf .turbo node_modules dist bin coverage *.tsbuildinfo || exit 0",
    "lint": "npx eslint . --ext ts,tsx --max-warnings 0 --fix",
    "dev": "remix vite:dev",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "build": "remix vite:build",
    "test": "npx jest --coverage --verbose",
    "start": "remix-serve build/server/index.js"
  },
  "dependencies": {
    "@dbbs/tailwind-components": "*",
    "@remix-run/node": "2.12.1",
    "@remix-run/react": "2.12.1",
    "@remix-run/serve": "2.12.1",
    "cheerio": "1.0.0-rc.12",
    "isbot": "4.1.0",
    "lodash.camelcase": "4.3.0",
    "lucide-react": "0.453.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sanitize-html": "2.13.1",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@dbbs/cypress": "*",
    "@dbbs/eslint-config": "*",
    "@dbbs/firebase": "*",
    "@dbbs/jest-config": "*",
    "@remix-run/dev": "2.12.1",
    "@remix-run/testing": "2.12.1",
    "@types/cheerio": "0.22.35",
    "@types/lodash.camelcase": "4",
    "@types/react-dom": "18.2.7",
    "@types/sanitize-html": "2",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.1",
    "vite-tsconfig-paths": "4.3.1"
  }
}
