{
  "name": "@dbbs/serverless-api",
  "version": "2.0.1",
  "description": "DBBs Serverless API",
  "type": "module",
  "main": "./dist/lambda.js",
  "types": "./dist/lambda.d.ts",
  "scripts": {
    "build": "npx tsc --build tsconfig.json",
    "clean": "npx rimraf .serverless .turbo node_modules dist bin coverage && find . -name '*.tsbuildinfo' -type f -delete || exit 0",
    "dev": "yarn build && npx sls offline start --noTimeout --stage local",
    "lint": "npx eslint . --ext ts,tsx --max-warnings 0 --fix",
    "start": "npx nest start",
    "pretest": "yarn build",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --forceExit --coverage --verbose --testTimeout=10000000 --detectOpenHandles",
    "watch": "npx tsc -w --preserveWatchOutput",
    "start:localstack": "yarn build && npx sls offline start --noTimeout --stage local",
    "docs:generate": "npx compodoc -p tsconfig.json",
    "docs:serve": "yarn docs:generate -s",
    "check-types": "npx tsc -p ./tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "@dbbs/common": "*",
    "@dbbs/feature-config": "*",
    "@dbbs/nestjs-module-authz": "*",
    "@dbbs/nestjs-module-health": "*",
    "@dbbs/nestjs-module-logger": "*",
    "@dbbs/nestjs-module-settings": "*",
    "@dbbs/s3-log-transport": "*",
    "@middy/core": "5.5.1",
    "@middy/do-not-wait-for-empty-event-loop": "5.5.1",
    "@middy/http-security-headers": "5.5.1",
    "@middy/input-output-logger": "5.5.1",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "10.4.6",
    "@nestjs/terminus": "10.2.3",
    "@serdnam/pino-cloudwatch-transport": "1.0.4",
    "@types/aws-lambda": "8.10.147",
    "@vendia/serverless-express": "4.12.6",
    "express": "4.21.1",
    "form-data": "4.0.1",
    "helmet": "8.0.0",
    "jwks-rsa": "3.1.0",
    "nestjs-cls": "4.4.1",
    "ow": "2.0.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@dbbs/tsconfig": "*"
  }
}
