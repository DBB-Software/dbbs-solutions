{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "display": "Next.js",
  "files": [],
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "**/*.d.ts"]
}