{
  "name": "@dbbs/platform",
  "private": true,
  "workspaces": {
    "packages": [
      "typescript/apps/*",
      "typescript/packages/*",
      "typescript/modules/*",
      "typescript/packages/strapi-plugins/*",
      "typescript/packages/nestjs-modules/*",
      "typescript/packages/mobile-storages/*",
      "typescript/packages/mobile-features/*",
      "typescript/packages/web-storages/*",
      "python/packages/django-modules/*",
      "python/packages/fast-api-modules/*",
      "python/packages/*",
      "python/apps/*",
      "infra/aws-cdk"
    ]
  },
  "scripts": {
    "build": "dotenv -- npx turbo run build --filter @dbbs/${target:-\"*\"}",
    "clean:all": "npx turbo run clean && npm run clean:modules",
    "clean:modules": "npx rimraf node_modules .yarn .turbo .idea",
    "cypress:open": "dotenv -- npx turbo run cypress:open --filter @dbbs/${target:-\"*\"}",
    "cypress:run": "dotenv -- npx turbo run cypress:run --filter @dbbs/${target:-\"*\"}",
    "dev": "dotenv -- npx turbo run dev --concurrency=99 --filter @dbbs/${target:-\"*\"}",
    "start": "dotenv -- npx turbo run start --concurrency=99 --filter @dbbs/${target:-\"*\"}",
    "download:env": "bash scripts/download.env.sh $STAGE",
    "upload:env": "bash scripts/upload-env.sh $STAGE",
    "lint": "yarn prettier && npx turbo run lint --filter @dbbs/${target:-\"*\"}",
    "prettier": "npx prettier --write \"typescript/packages/**/*.{ts,tsx}\" \"typescript/apps/**/*.{ts,tsx}\" \"infra/**/*.{ts,tsx}\"",
    "sort-packages": "npm i -g sort-package-json && sort-package-json && find packages/* apps/* -name package.json -maxdepth 2 -execdir npx sort-package-json \\; -execdir rm -rf node_modules \\;",
    "test": "dotenv -- npx turbo run test --filter @dbbs/${target:-\"*\"}",
    "e2e": "dotenv -- npx turbo run e2e --filter @dbbs/${target:-\"*\"}",
    "update-packages": "npm i -g npm-check-updates && ncu -u && find packages/* apps/* -name package.json -maxdepth 2 -execdir npx ncu -u \\; -execdir rm -rf node_modules \\;",
    "start:localstack": "make localstack-up && turbo run start:localstack",
    "upload:settings": "./scripts/upload-settings.sh",
    "migrate": "dotenv -- npx turbo run migrate --concurrency=99 --filter @dbbs/${target:-\"*\"}",
    "test:cov": "dotenv -- npx turbo run test:cov --concurrency=99 --filter @dbbs/${target:-\"*\"}",
    "prepare": "husky",
    "check-types": "npx turbo run check-types"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/preset-env": "7.24.5",
    "@babel/runtime": "7.24.5",
    "@compodoc/compodoc": "1.1.26",
    "@dbbs/eslint-config": "*",
    "@dbbs/tsconfig": "*",
    "@jest/globals": "29.7.0",
